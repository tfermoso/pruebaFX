<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="356.0" prefWidth="673.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label prefHeight="18.0" prefWidth="604.0" text="Nombre de usuario" />
   <TextField fx:id="txtUsuario" prefHeight="26.0" prefWidth="208.0" />
   <Label prefHeight="18.0" prefWidth="621.0" text="Password" />

   <PasswordField fx:id="txtPassword" />

    <Label fx:id="welcomeText" />
    <Button onAction="#onLoginClick" prefHeight="26.0" prefWidth="126.0" text="Login" />
</VBox>
